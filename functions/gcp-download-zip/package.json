{
  "name": "@bmi/gcp-download-zip",
  "private": true,
  "version": "0.0.0",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && functions-framework --source=dist --target=download --port=9704",
    "deploy": "tsc && gcloud functions deploy file-download --trigger-http --runtime=nodejs10 --allow-unauthenticated --max-instances=5 --region=europe-west3 --entry-point=download"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "jszip": "^3.5.0",
    "node-fetch": "^2.6.1"
  }
}
