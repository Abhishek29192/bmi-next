{
  "name": "@bmi/gcp-download-zip",
  "private": true,
  "version": "0.0.0",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist --target=download --port=9704",
    "deploy": "tsc && gcloud functions deploy download-zip --trigger-http --runtime=nodejs10 --allow-unauthenticated --max-instances=5 --region=europe-west3 --entry-point=download --env-vars-file=.env.yaml",
    "coverage:jest": "jest --coverage"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@google-cloud/secret-manager": "^3.4.0",
    "@google-cloud/storage": "^5.7.0",
    "archiver": "^5.1.0",
    "jszip": "^3.5.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "js-yaml": "^4.0.0"
  }
}
