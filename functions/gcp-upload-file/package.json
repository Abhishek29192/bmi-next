{
  "name": "@bmi/gcp-upload-file",
  "private": true,
  "version": "0.0.0",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && functions-framework --source=dist --target=upload --port=7040",
    "deploy": "tsc && gcloud functions deploy upload-file --trigger-http --runtime=nodejs10 --allow-unauthenticated --max-instances=5 --region=europe-west3 --entry-point=upload --env-vars-file=env.yaml"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "buffer": "^5.6.0",
    "contentful-management": "^6.1.2",
    "dotenv": "^8.2.0"
  }
}
