{
  "name": "@bmi/gcp-build-trigger",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "compile": "tsc",
    "test": "jest --coverage --reporters=default --reporters=jest-junit",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist/dist/function --target=build --port=9057",
    "dev:top": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json process-top ./node_modules/.bin/functions-framework --source=dist/dist/function --target=build --port=9057",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "^1.3.3",
    "@google-cloud/functions-framework": "^3.0.0",
    "@google-cloud/monitoring": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "env-cmd": "^10.1.0",
    "generate-package-json-webpack-plugin": "^2.6.0",
    "inspectpack": "^4.7.1",
    "jest-when": "^3.2.0",
    "js-yaml": "^4.0.0",
    "process-top": "^1.2.0",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
