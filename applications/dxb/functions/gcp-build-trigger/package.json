{
  "name": "@bmi/gcp-build-trigger",
  "version": "0.0.0",
  "license": "BMI-DXB",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "check:ts": "tsc",
    "coverage:jest": "jest --coverage --reporters=default --reporters=jest-junit",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist/dist/function --target=build --port=9057",
    "dev:top": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json process-top ./node_modules/.bin/functions-framework --source=dist/dist/function --target=build --port=9057"
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "1.1.1",
    "@google-cloud/functions-framework": "^1.7.1",
    "@google-cloud/monitoring": "^2.3.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^10.2.4",
    "env-cmd": "^10.1.0",
    "generate-package-json-webpack-plugin": "^2.6.0",
    "inspectpack": "^4.7.1",
    "jest-when": "^3.2.0",
    "js-yaml": "^4.0.0",
    "process-top": "^1.2.0",
    "webpack": "^5.33.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
