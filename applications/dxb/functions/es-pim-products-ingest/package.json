{
  "name": "@bmi/es-pim-products-ingest",
  "private": true,
  "version": "0.0.0",
  "license": "BMI-DXB",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "check:ts": "tsc",
    "coverage:jest": "jest --coverage --reporters=default --reporters=jest-junit",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist --signature-type=event --target=handleMessage --port=9049",
    "dev:top": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json process-top ./node_modules/.bin/functions-framework --source=dist --signature-type=event --target=handleMessage --port=9049"
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "1.1.1",
    "@bmi/pim-types": "0.0.0",
    "@google-cloud/functions-framework": "^1.7.1",
    "@elastic/elasticsearch": "^7.8.0",
    "@bmi/gcp-pim-message-handler": "0.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "inspectpack": "^4.7.1",
    "js-yaml": "^4.0.0",
    "process-top": "^1.2.0",
    "webpack": "^5.33.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
