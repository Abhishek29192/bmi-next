{
  "name": "@bmi/gcp-pim-full-fetch",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "compile": "tsc",
    "test": "jest --coverage --reporters=default --reporters=jest-junit",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist/dist/function --target=handleRequest --port=9055",
    "dev:top": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json process-top ./node_modules/.bin/functions-framework --source=dist/dist/function --target=handleRequest --port=9055",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "^1.6.0",
    "@bmi/functions-contentful-client": "0.0.0",
    "@bmi/functions-es-client": "0.0.0",
    "@bmi/pim-api": "0.0.0",
    "@google-cloud/pubsub": "^2.5.0",
    "contentful": "^9.2.5",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@bmi/contentful-types": "0.0.0",
    "@bmi/elasticsearch-types": "0.0.0",
    "@bmi/pim-types": "0.0.0",
    "@bmi/pub-sub-types": "0.0.0",
    "@google-cloud/functions-framework": "^3.0.0",
    "env-cmd": "^10.1.0",
    "inspectpack": "^4.7.1",
    "js-yaml": "^4.0.0",
    "process-top": "^1.2.0",
    "webpack": "^5.80.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.2"
  }
}
