{
  "name": "@bmi/gcp-functions-deployer",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run compile && webpack --mode=production",
    "compile": "tsc --build",
    "dev": "yarn build && env-cmd functions-framework --source=dist --target=deploy --port=9054 --signature-type=event",
    "dev:top": "yarn build && env-cmd process-top $(yarn bin functions-framework) --source=dist --target=deploy --port=9054 --signature-type=event",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "jest --coverage --reporters=default --reporters=jest-junit"
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "^1.7.5",
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/storage": "^7.0.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/jest-when": "^3.5.2",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.13.0",
    "inspectpack": "^4.7.1",
    "jest": "^29.6.4",
    "jest-when": "^3.6.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.1.1",
    "process-top": "^1.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  }
}
