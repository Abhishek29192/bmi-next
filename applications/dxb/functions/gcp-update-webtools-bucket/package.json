{
  "name": "@bmi/gcp-update-webtools-bucket",
  "version": "0.0.0",
  "license": "BMI-DXB",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "compile": "tsc",
    "test": "jest --coverage --reporters=default --reporters=jest-junit",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist --target=handleRequest --port=9058",
    "dev:top": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json process-top ./node_modules/.bin/functions-framework --source=dist --target=handleRequest --port=9058",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "1.2.0",
    "@bmi-digital/functions-secret-client": "1.1.1",
    "@google-cloud/functions-framework": "^3.0.0",
    "@google-cloud/storage": "^5.7.0",
    "joi": "^17.4.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "inspectpack": "^4.7.1",
    "js-yaml": "^4.0.0",
    "process-top": "^1.2.0",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
