{
  "name": "@bmi/gcp-firestore-writer",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "compile": "tsc",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist --target=handleMessage --signature-type=event",
    "dev:debug": "webpack --mode=development && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json node --inspect ./node_modules/.bin//functions-framework --source=dist --target=handleMessage --signature-type=event --port 8090",
    "dev:top": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json process-top ./node_modules/.bin/functions-framework --source=dist --target=handleMessage",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "jest --coverage --reporters=default --reporters=jest-junit"
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "^1.6.0",
    "@bmi/firestore-types": "0.0.0",
    "@bmi/functions-firestore": "0.0.0",
    "@bmi/pim-types": "0.0.0",
    "@bmi/utils": "0.0.0",
    "@google-cloud/functions-framework": "^3.0.0",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@bmi/firestore-types": "0.0.0",
    "@bmi/pim-types": "0.0.0",
    "@bmi/pub-sub-types": "0.0.0",
    "env-cmd": "^10.1.0",
    "inspectpack": "^4.7.1",
    "js-yaml": "^4.0.0",
    "process-top": "^1.2.0",
    "webpack": "^5.80.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.2"
  }
}
