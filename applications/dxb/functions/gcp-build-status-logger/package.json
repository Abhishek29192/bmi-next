{
  "name": "@bmi/gcp-build-status-logger",
  "private": true,
  "version": "0.0.0",
  "license": "BMI-DXB",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run compile && webpack --mode=production",
    "test": "jest --coverage --reporters=default --reporters=jest-junit",
    "compile": "tsc --build",
    "dev": "yarn build && env-cmd functions-framework --source=dist --target=buildStatusLogger --port=9061",
    "dev:top": "yarn build && env-cmd process-top $(yarn bin functions-framework) --source=dist/dist/function --target=buildStatusLogger --port=9061",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "^1.7.5",
    "@bmi/functions-firestore": "workspace:*",
    "@google-cloud/functions-framework": "^3.3.0",
    "dotenv": "^16.0.3",
    "googleapis": "^118.0.0",
    "node-fetch": "^2.6.11"
  },
  "devDependencies": {
    "@bmi/firestore-types": "workspace:*",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "inspectpack": "^4.7.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "process-top": "^1.2.0",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.1"
  }
}
