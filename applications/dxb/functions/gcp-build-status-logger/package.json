{
  "name": "@bmi/gcp-build-status-logger",
  "private": true,
  "version": "0.0.0",
  "license": "BMI-DXB",
  "src": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "test": "jest --coverage --reporters=default --reporters=jest-junit",
    "compile": "tsc",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist/dist/function --target=buildStatusLogger --port=9061",
    "dev:top": "yarn build && js-yaml ./.env.yaml > ./.env.json && process-top ./node_modules/.bin/functions-framework --source=dist/dist/function --target=buildStatusLogger --port=9061"
  },
  "dependencies": {
    "@bmi/functions-firestore": "0.0.0",
    "@bmi-digital/functions-logger": "1.1.1",
    "@bmi-digital/functions-secret-client": "1.1.1",
    "@google-cloud/functions-framework": "^3.0.0",
    "dotenv": "^8.2.0",
    "googleapis": "67.1.1",
    "node-fetch": "^2.6.1",
    "simple-http-status": "1.1.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "inspectpack": "^4.7.1",
    "js-yaml": "^4.0.0",
    "process-top": "^1.2.0",
    "webpack": "^5.33.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
