{
  "name": "@bmi/gcp-es-proxy",
  "private": true,
  "description": "Simple NodeJS proxy to query Elastic Search",
  "version": "0.0.0",
  "keywords": [
    "node",
    "elastic search"
  ],
  "license": "BMI-DXB",
  "scripts": {
    "build": "webpack --mode=production",
    "check:ts": "tsc",
    "coverage:jest": "jest --coverage --reporters=default --reporters=jest-junit",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist --target=proxy --port=9052",
    "dev:top": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json process-top ./node_modules/.bin/functions-framework --source=dist --target=proxy --port=9052"
  },
  "dependencies": {
    "@bmi-digital/functions-logger": "1.1.1",
    "@google-cloud/functions-framework": "^2.1.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.2",
    "inspectpack": "^4.7.1",
    "js-yaml": "^4.0.0",
    "process-top": "^1.2.0",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
