{
  "name": "@bmi/gcp-es-proxy",
  "private": true,
  "description": "Simple NodeJS proxy to query Elastic Search",
  "version": "0.0.0",
  "keywords": [
    "node",
    "elastic search"
  ],
  "license": "BMI-DXB",
  "scripts": {
    "build": "webpack --mode=production",
    "check:ts": "tsc",
    "coverage:jest": "jest --coverage --reporters=default --reporters=jest-junit",
    "dev": "yarn build && js-yaml ./.env.yaml > ./.env.json && env-cmd -r ./.env.json functions-framework --source=dist --target=proxy --port=9052"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.2",
    "env-cmd": "^10.1.0",
    "js-yaml": "^4.0.0"
  }
}
