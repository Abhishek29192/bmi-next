  "scripts": {
    "build": "gatsby build --prefix-paths",
    "build:top": "process-top $(yarn bin) build",
    "compile": "tsc --build",
    "clean": "gatsby clean",
    "contentful-graph": "dotenv -c development -- contentful-graph",
    "dev": "gatsby clean && gatsby develop",
    "debug": "node --nolazy --inspect-brk node_modules/.bin/gatsby develop",
    "lighthouse": "yarn lhci autorun",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "microcopy": "node ./scripts/get-microcopy.js",
    "performance": "cross-env NODE_ENV=production PERFORMANCE_ANALYTICS=true GATSBY_OPEN_TRACING_CONFIG_FILE=./node_modules/gatsby-build-newrelic/zipkin-local.js yarn build --graphql-tracing",
    "start": "gatsby build --prefix-paths && gatsby serve",
    "test": "jest --coverage --reporters=default --reporters=jest-junit",
    "postinstall": "if-env GATSBY_CLOUD=true && run compile || echo 'do nothing'"
  },
  "workspaces": {
    "nohoist": [
      "gatsby-build-newrelic"
    ]
  },
