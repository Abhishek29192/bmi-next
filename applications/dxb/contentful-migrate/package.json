{
  "name": "@bmi/contentful-migrate",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "src": "src/index.ts",
  "scripts": {
    "build": "tsc --build && cp -R './src/images' ./dist/images",
    "compile": "tsc --build",
    "migrate-create": "node ./dist/create.js",
    "debug": "node --inspect ./dist/index.js",
    "update-mc": "node ./dist/cma/update-mc.js",
    "update-mc:publish": "node ./dist/cma/update-mc.js --publish",
    "migrate-down": "node ./dist/down.js",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "start": "node ./dist/index.js",
    "test": "node --experimental-vm-modules $(yarn bin jest) --coverage --reporters=default --reporters=jest-junit",
    "test:debug": "node --experimental-vm-modules --inspect $(yarn bin jest) --runInBand",
    "migrate-up": "node ./dist/up.js"
  },
  "dependencies": {
    "@bmi-digital/contentful-migration": "^1.4.0",
    "@bmi/functions-contentful-management-client": "workspace:*",
    "@bmi/microcopies": "workspace:*",
    "@bmi/utils": "workspace:*",
    "contentful-management": "^10.39.2",
    "lodash-es": "^4.17.21",
    "migrate": "^2.0.0",
    "ora": "^6.3.1",
    "semver-parser": "^4.1.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
