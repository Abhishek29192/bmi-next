{
  "name": "@bmi/dxb-contentful-migrate",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "src": "src/index.ts",
  "scripts": {
    "build": "tsc --build && copyfiles './src/images/**/*' ./dist/contentful-migrate",
    "compile": "tsc",
    "migrateCreate": "node ./dist/contentful-migrate/src/create.js",
    "debug": "node --inspect ./dist/contentful-migrate/src/index.js",
    "dxb-update-mc": "node ./dist/contentful-migrate/src/cma/update-mc.js",
    "dxb-update-mc:publish": "node ./dist/contentful-migrate/src/cma/update-mc.js --publish",
    "down": "node ./dist/contentful-migrate/src/down.js",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "start": "node ./dist/contentful-migrate/src/index.js",
    "test": "jest --coverage --reporters=default --reporters=jest-junit",
    "up": "node ./dist/contentful-migrate/src/up.js"
  },
  "dependencies": {
    "@bmi-digital/contentful-migration": "^1.2.2",
    "ora": "^5.3.0",
    "semver-parser": "^3.0.5"
  },
  "devDependencies": {
    "contentful-management": "^10.30.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "jest": "^29.4.3",
    "jest-junit": "^15.0.0",
    "ts-jest": "^29.0.5"
  }
}
