{
  "name": "@bmi/dxb-contentful-migrate",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "src": "src/index.ts",
  "scripts": {
    "build": "tsc --build && copyfiles './src/images/**/*' ./dist/contentful-migrate",
    "compile": "tsc --build",
    "migrateCreate": "node ./dist/contentful-migrate/src/create.js",
    "debug": "node --inspect ./dist/contentful-migrate/src/index.js",
    "dxb-update-mc": "node ./dist/contentful-migrate/src/cma/update-mc.js",
    "dxb-update-mc:publish": "node ./dist/contentful-migrate/src/cma/update-mc.js --publish",
    "down": "node ./dist/contentful-migrate/src/down.js",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "start": "node ./dist/contentful-migrate/src/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --reporters=default --reporters=jest-junit",
    "test:debug": "node --experimental-vm-modules --inspect $(yarn bin jest) --runInBand",
    "up": "node ./dist/contentful-migrate/src/up.js"
  },
  "dependencies": {
    "@bmi-digital/contentful-migration": "^1.2.2",
    "@bmi/utils": "workspace:*",
    "contentful-management": "^10.35.3",
    "ora": "^6.3.0",
    "semver-parser": "^4.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
