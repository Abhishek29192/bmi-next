                <loginurl>REPLACE_ME</loginurl>
                <loginbody>username={%username%}&amp;password={%password%}</loginbody>
                <loginpageurl>https://bmi-dxb-head-qa.netlify.app/</loginpageurl>
            </form>
        </authentication>
        <users>
            <user>1388;true;cWE=;2;cWE=~TDN0TTNpbmQkYjIwMjA=~</user>
        </users>
        <forceduser>1388</forceduser>
        <session>
            <type>0</type>
        </session>
        <authorization>
            <type>0</type>
            <basic>
                <header/>
                <body/>
                <logic>AND</logic>
                <code>-1</code>
            </basic>
        </authorization>
    </context>
</configuration>
