{
  "name": "@bmi/intouch-frontend",
  "version": "0.0.0",
  "private": true,
  "license": "BMI-DXB",
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "codegen": "graphql-codegen -r dotenv/config --config codegen.yml",
    "codegen:watch": "yarn codegen --watch \"**/!(generated)/*.(ts|tsx|graphql)\"",
    "coverage:jest": "jest --coverage --reporters=default --reporters=jest-junit",
    "dev": "concurrently \"next dev\" \"yarn codegen:watch\"",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s ./public",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@auth0/nextjs-auth0": "^1.3.0",
    "@bmi/button": "0.0.0",
    "@bmi/card": "0.0.0",
    "@bmi/company-details": "0.0.0",
    "@bmi/intouch-api-types": "0.0.0",
    "@bmi/logger": "0.0.0",
    "@bmi/typography": "0.0.0",
    "@google-cloud/secret-manager": "^3.6.0",
    "@next/bundle-analyzer": "^10.1.3",
    "@svgr/webpack": "^4.3.3",
    "@types/webpack": "^4.32.2",
    "concurrently": "^6.2.0",
    "css-loader": "^3.2.0",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^7.0.2",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.2.0",
    "isomorphic-style-loader": "^5.1.0",
    "mini-css-extract-plugin": "^1.5.1",
    "next": "10.2.0",
    "next-connect": "^0.10.1",
    "next-fonts": "^1.5.1",
    "next-i18next": "8.2.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.0.0",
    "uuid": "^8.3.2",
    "webpack": "^5.33.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/import-types-preset": "^1.18.2",
