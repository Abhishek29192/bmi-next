{
  "name": "@bmi/gcp-intouch-handle-message",
  "version": "0.0.0",
  "main": "dist/index.js",
  "description": "BMI InTouch Function Handle Pub/Sub message",
  "license": "BMI-DXB",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
