{
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "editor.rulers": [80],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "spellright.language": ["en"],
  "spellright.documentTypes": ["latex", "plaintext"],
  "[sql]": {
    "editor.formatOnSave": false
  },
  "jest.jestCommandLine": "yarn jest --config=jest.config.js --maxWorkers=4"
}
