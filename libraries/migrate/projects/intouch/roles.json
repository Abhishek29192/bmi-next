[
  {
    "name": "Market Admin",
    "description": "The market admin in Contentful is in charge of creating and editing entries",
    "policies": [
      {
        "effect": "allow",
        "actions": "all",
        "constraint": {
          "and": [
            {
              "equals": [
                {
                  "doc": "sys.type"
                },
                "Entry"
              ]
            }
          ]
        }
      },
      {
        "effect": "allow",
        "constraint": {
          "and": [
            {
              "equals": [
                {
                  "doc": "sys.type"
                },
                "Asset"
              ]
            }
          ]
        },
        "actions": "all"
      }
    ],
    "permissions": {
      "ContentDelivery": "all",
      "ContentModel": ["read"],
      "EnvironmentAliases": [],
      "Environments": [],
      "Settings": [],
      "Tags": []
    }
  }
]
